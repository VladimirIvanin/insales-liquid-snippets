'.liquid':
  'Liquid replace':
    'prefix': 'repl'
    'body': " | replace: '${1:substring}', '${2:replacement}'"

  'Liquid asset (local)':
    'prefix': 'assu'
    'body': """
    {{ '${1}' | ${2}asset_url }}
    """

  'Liquid img asset (local)':
    'prefix': 'imgass'
    'body': """
    <img src="{{ '${1}' | asset_url }}" alt="${2}">
    """

  'Common js search':
    'prefix': 'search'
    'body': """
    <form action="/search" method="get">
      <input type="hidden" name="lang" value="{{ language.locale }}">
      <input type="text" name="q" value="" placeholder="Поиск" data-search-field />
      <button type="submit">Поиск</button>
      <div data-search-result></div>
    </form>
    """

  'Common js product':
    'prefix': 'prodform'
    'body': """
    <form action="{{ cart_url }}" method="post" data-product-id="{{ product.id }}">
      {% if product.show_variants? %}
        <select name="variant_id" data-product-variants>
          {% for variant in product.variants %}
            <option value="{{ variant.id }}">{{ variant.title | escape }}</option>
          {% endfor %}
        </select>
      {% else %}
        <input type="hidden" name="variant_id" value="{{product.variants.first.id}}" >
      {% endif %}
      <input type="text" name="comment" value="">
      <div data-quantity>
        <input type="text" name="quantity" value="1" />
        <span data-quantity-change="-1">-</span>
        <span data-quantity-change="1">+</span>
      </div>
      <button type="submit" data-item-add>
        Добавить в корзину
      </button>
    </form>
    """

  'Liquid paginate by':
    'prefix': 'pagby'
    'body': """
    {% paginate ${1} by ${2} %}
      ${3}
    {% endpaginate %}
    """

  'Liquid for product in collection.products':
    'prefix': 'forprodincp'
    'body': """
    {% for product in collection.products %}
      ${1}
    {% endfor %}
    """

  'Liquid If Collection Products Size':
    'prefix': 'ifcps'
    'body': """
    {% if collection.products.size > 0 %}
      ${1}
    {% endif %}
    """

  'Liquid form':
    'prefix': 'form'
    'body': """
    {% form '${1}', ${2} %}
      ${3}
    {% endform %}
    """

  'Liquid include with':
    'prefix': 'incwith'
    'body': """
    {% include "${1}" with ${2} %}
    """

  'Liquid include':
    'prefix': 'inc'
    'body': """
    {% include "${1}" %}
    """

  'Liquid capture':
    'prefix': 'cap'
    'body': """
    {% capture ${1} %}
    ${2}
    {% endcapture %}
    """

  'Liquid assign':
    'prefix': 'ass'
    'body': """
    {% assign ${1} = ${2} %}
    """

  'Liquid tablerow':
    'prefix': 'tabrow'
    'body': """
    {% tablerow ${1} %}
      ${2}
    {% endtablerow %}
    """

  'Liquid for':
    'prefix': 'for'
    'body': """
    {% for ${1} %}
      ${2}
    {% endfor %}
    """

  'Liquid cycle':
    'prefix': 'cycle'
    'body': """
    {% cycle '${1}' %}
    """

  'Liquid case':
    'prefix': 'case'
    'body': """
    {% case ${1} %}
      {% when ${2} %}
        ${3}
      {% else %}
        ${4}
    {% endcase %}
    """

  'Liquid unless (inline)':
    'prefix': 'unlin'
    'body': '{% unless ${1} %}${2}{% endunless %}'

  'Liquid unless':
    'prefix': 'unl'
    'body': """
    {% unless ${1} %}
      ${2}
    {% endunless %}
    """

  'Liquid if / elseif':
    'prefix': 'ifelif'
    'body': """
    {% if ${1} %}
      ${2}
    {% elsif ${3} %}
      ${4}
    {% endif %}
    """

  'Liquid if':
    'prefix': 'if'
    'body': """
    {% if ${1} %}
      ${2}
    {% endif %}
    """

  'Liquid raw':
    'prefix': 'raw'
    'body': """
    {% raw %} ${1} {% endraw %}
    """

  'Liquid comment':
    'prefix': 'com'
    'body': """
    {% comment %}
      ${1}
    {% endcomment %}
    """

  'Liquid product.id':
    'prefix': 'prodid'
    'body': '{{ product.id }}'

  'Liquid product.title':
    'prefix': 'prodtitle'
    'body': '{{ product.title }}'

  'Liquid product.handle':
    'prefix': 'prodhandle'
    'body': '{{ product.handle }}'

  'Liquid product.type':
    'prefix': 'prodtype'
    'body': '{{ product.type }}'

  'Liquid product.vendor':
    'prefix': 'prodvendor'
    'body': '{{ product.vendor }}';

  'Liquid product.price':
    'prefix': 'prodprice'
    'body': '{{ product.price }}'

  'Liquid product.price_min':
